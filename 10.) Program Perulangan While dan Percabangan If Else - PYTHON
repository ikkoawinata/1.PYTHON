

##FUNSGI SELAMAT DATANG
def welcome():
    print('================================================================')
    print('               S E L A M A T   D A T A N G   D I                ')
    print('             PERULANGAN WHILE DAN PERCANGAN IF ELSE             ')
    print('================================================================')
welcome()

umur = int(input("Umur Anda Berapa? "))

while umur < 5 or umur >= 500:
    print("Saya tidak yakin umur anda", umur, "tahun")
    print("Silahkan diulangi...\n")

    umur = int(input("Umur Anda Berapa? "))
print()

if umur > 12 and umur < 19:
    print("Wahh anda baru beranjak remaja")
    print("Gunakan waktu sebaik-baiknya ya")

if umur >= 19 and umur <= 100:
    print("Anda sudah beranjak tua")
    print("Jadilah seseorang yang dewasa")

elif umur > 100 and umur <= 500:
    print("WOAAH ANDA SANGATTTTT WAAHH SEKALI")
    
    
    print("terima kasih telah menggunakan program ini")
