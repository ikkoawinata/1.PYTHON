def welcome():
  print("=========================================================")
  print("   S E L A M A T  D A T A N G  D I  P R O G R A M ")
  print("MEMBANDINGKAN BILANGAN INTEGER DAN MENGELUARKAN PESAN")
  print("=========================================================")
  
welcome()
a = float(input("Masukkan nilai integer A : "))
b = float(input("Masukkan nilai integer B : "))
##pernyataan


    
if (a > b ):
    print("Nilai A lebih besar dari nilai B")
elif (a < b):
    print("Nilai A lebih kecil dari nilai B")
elif (a == b):
    print("Nilai A sama dengan nilai B")
