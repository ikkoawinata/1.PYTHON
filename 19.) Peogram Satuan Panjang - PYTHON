def sambutan():
    print ("================================================================================")
    print ("       S E L A M A T   D A T A N G   D I   P R O G R A M " )
    print ("                  CONVERT SATUAN PANJANG " )


def table():
    print ("================================================================================")
    print ("NO |                 SATUAN PANJANG               ")
    print ("================================================================================")
    print ("1.  | KM (Kilometer)       ->   HM (Hektometer)                                 |")
    print ("2.  | KM (Kilometer)       ->   DAM (Dekameter)                                 |")
    print ("3.  | KM (Kilometer)       ->   M (Meter)                                       |")
    print ("4.  | KM (Kilometer)       ->   DM (Desimeter)                                  |")
    print ("5.  | KM (Kilometer)       ->   CM (Centimeter)                                 |")
    print ("6.  | KM (Kilometer)       ->   MM (Milimeter)                                  |")
    print ("7.  | HM (Hektometer)      ->   KM (Kilometer)                                  |")
    print ("8.  | HM (Hektometer)      ->   DAM (Dekameter)                                 |")
    print ("9.  | HM (Hektometer)      ->   M (Meter)                                       |")
    print ("10. | HM (Hektometer)      ->   DM (Desimeter)                                  |")
    print ("11. | HM (Hektometer)      ->   CM (Centimeter)                                 |")
    print ("12. | HM (Hektometer)      ->   MM (Milimeter)                                  |")
    print ("13. | DAM (Dekameter)      ->   KM (Kilometer)                                  |")
    print ("14. | DAM (Dekameter)      ->   HM (Hektometer)                                 |")
    print ("15. | DAM (Dekameter)      ->   M (Meter)                                       |")
    print ("16. | DAM (Dekameter)      ->   DM (Desimeter)                                  |")
    print ("17. | DAM (Dekameter)      ->   CM (Centimeter)                                 |")
    print ("18. | DAM (Dekameter)      ->   MM (Milimeter)                                  |")
    print ("19. | M (Meter)            ->   KM (Kilometer)                                  |")
    print ("20. | M (Meter)            ->   HM (Hektometer)                                 |")
    print ("21. | M (Meter)            ->   DAM (Dekameter)                                 |")
    print ("22. | M (Meter)            ->   DM (Desimeter)                                  |")
    print ("23. | M (Meter)            ->   CM (Centimeter)                                 |")
    print ("24. | M (Meter)            ->   MM (Milieter)                                   |")
    print ("25. | DM (Desimeter)       ->   KM (Kilometer)                                  |")
    print ("26. | DM (Desimeter)       ->   HM (Hektometer)                                 |")
    print ("27. | DM (Desimeter)       ->   DAM (Dekameter)                                 |")
    print ("28. | DM (Desimeter)       ->   M (Meter)                                       |")
    print ("29. | DM (Desimeter)       ->   CM (Centimeter)                                 |")
    print ("30. | DM (Desimeter)       ->   MM (Milimeter)                                  |")
    print ("31. | CM (Centimeter)      ->   KM (Kilometer)                                  |")
    print ("32. | CM (Centimeter)      ->   HM (Hektometer)                                 |")
    print ("33. | CM (Centimeter)      ->   DAM (Dekameter)                                 |")
    print ("34. | CM (Centimeter)      ->   M (Meter)                                       |")
    print ("35. | CM (Centimeter)      ->   DM (Desimeter)                                  |")
    print ("36. | CM (Centimeter)      ->   MM (Milimeter)                                  |")
    print ("37. | MM (Milimeter)       ->   KM (Kilometer)                                  |")
    print ("38. | MM (Milimeter)       ->   HM (Hektometer)                                 |")
    print ("39. | MM (Milimeter)       ->   DAM (Dekameter)                                 |")
    print ("40. | MM (Milimeter)       ->   M (Meter)                                       |")
    print ("41. | MM (Milimeter)       ->   DM (Desimeter)                                  |")
    print ("42. | MM (Milimeter)       ->   CM (Centimeter)                                 |")
    print ("================================================================================")

sambutan()
table()

